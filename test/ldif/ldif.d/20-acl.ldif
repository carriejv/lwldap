dn: olcDatabase={1}mdb,cn=config
changetype: modify
replace: olcAccess
olcAccess: {0} to *
    by dn.base="cn=root,dc=example,dc=com" manage
    by * break
olcAccess: {1} to attrs=userPassword,loginShell
    by self write
    by anonymous auth
    by * none
olcAccess: {2} to attrs=cn,sn,givenName,telephoneNumber,mail,loginShell,photo,jpegPhoto
    by self write
    by anonymous auth
    by users read
olcAccess: {3} to *
    by * read
